{
  "name": "debug-custom",
  "version": "1.2.0",
  "description": "A thin wrapper around the debug logging package",
  "files": [
    "lib/index.js"
  ],
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "tap test/*.js --no-coverage  --test-env=DEBUG_HIDE_DATE=yes",
    "test:cov": "tap test/*.js --coverage  --test-env=DEBUG_HIDE_DATE=yes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmacnaughton/custom-debug.git"
  },
  "keywords": [
    "log",
    "logging",
    "debug",
    "simplified",
    "custom",
    "simple"
  ],
  "author": "Bruce A. MacNaughton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmacnaughton/custom-debug/issues"
  },
  "homepage": "https://github.com/bmacnaughton/custom-debug#readme",
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "tap": "^16.3.0"
  }
}
